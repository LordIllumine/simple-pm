<div class="card">
  <div class="card-body">
    <h2 class='card-title d-flex justify-content-between'>
      <span>Proyectos</span>
      <%= link_to 'Nuevo Proyecto', new_project_path, class: 'btn btn-primary' %>
    </h2>

    <% @projects.each do |project| %>
      <%= link_to project do %>
        <div class="card">
            <div class="card-body">
            
              <h5 class= 'card-title'><%= project.name %></h5>
              <p><%= project.description %></p>

              <p><h5 class= 'card-title'>Propietario del proyecto</h5></p>
              <p><%= project.user.name %> </p>
              

              <p><h5 class= 'card-title'>n√∫mero interno:</h5></p>
              <p><%= project.user_id %> </p>
               
              
            </div>
        </div>
     <% end %>
    <% end %>
  </div>
</div>
